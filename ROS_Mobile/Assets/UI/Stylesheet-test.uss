.ModeButtons {
    font-size: 65px;
    margin-right: 15px;
    flex-shrink: 1;
    flex-grow: 1;
    max-width: 60%;
    background-color: rgb(0, 31, 255);
    color: rgb(255, 255, 255);
    -unity-font: url('project://database/Assets/TextMesh%20Pro/Fonts/LiberationSans.ttf?fileID=12800000&guid=e3265ab4bf004d28a9537516768c1c75&type=3#LiberationSans');
    -unity-font-definition: initial;
    -unity-font-style: normal;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
    border-top-width: 3px;
    border-right-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
}

.ButtonDrive {
    min-width: 100%;
    flex-grow: 1;
    font-size: 65px;
    flex-shrink: 1;
    margin-bottom: 38px;
    -unity-font: url('project://database/Assets/TextMesh%20Pro/Fonts/LiberationSans.ttf?fileID=12800000&guid=e3265ab4bf004d28a9537516768c1c75&type=3#LiberationSans');
    -unity-font-definition: initial;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
}

.incDecButton {
    position: relative;
    height: 100%;
    width: 20%;
    left: 0;
    right: -69%;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    font-size: 100px;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
    background-color: rgb(0, 31, 255);
    color: rgb(255, 255, 255);
}

#unity-progress-bar {
    height: 50px;
    min-height: 50px;
}

.unity-progress-bar__progress {
    background-color: rgb(0, 255, 0);
}
