.ModeButtons {
    font-size: 65px;
    margin-right: 15px;
    flex-shrink: 1;
    flex-grow: 1;
    max-width: 60%;
    background-color: rgb(0, 31, 255);
    color: rgb(255, 255, 255);
    -unity-font: url('project://database/Assets/TextMesh%20Pro/Fonts/LiberationSans.ttf?fileID=12800000&guid=e3265ab4bf004d28a9537516768c1c75&type=3#LiberationSans');
    -unity-font-definition: initial;
    -unity-font-style: normal;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
    border-top-width: 3px;
    border-right-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
}

.ButtonDrive {
    min-width: 100%;
    flex-grow: 1;
    font-size: 65px;
    flex-shrink: 1;
    margin-bottom: 38px;
    -unity-font: url('project://database/Assets/TextMesh%20Pro/Fonts/LiberationSans.ttf?fileID=12800000&guid=e3265ab4bf004d28a9537516768c1c75&type=3#LiberationSans');
    -unity-font-definition: initial;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
}

.incDecButton {
    position: relative;
    height: 100%;
    width: 20%;
    left: 0;
    right: -69%;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    font-size: 100px;
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
    background-color: rgb(0, 31, 255);
    color: rgb(255, 255, 255);
}

#unity-progress-bar {
    height: 50px;
    min-height: 50px;
}

.unity-progress-bar__progress {
    background-color: rgb(0, 255, 0);
}

.popup_container {
    position: absolute;
    top: 260px;
    left: 225px;
    width: 60%;
    height: 50%;
    align-items: center;
    align-content: center;
    justify-content: center;
    display: none;
}

.popupWindow {
    height: 100%;
    width: 100%;
    background-color: rgb(150, 150, 150);
    border-top-width: 4px;
    border-right-width: 4px;
    border-bottom-width: 4px;
    border-left-width: 4px;
    border-top-color: rgb(0, 0, 0);
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
}

.horizontalLayout {
    flex-direction: row;
    justify-content: space-around;
    flex-wrap: wrap;
    align-items: flex-end;
    align-self: center;
    flex-shrink: 0;
    white-space: normal;
}

.warning_message {
    font-size: 50px;
    color: rgb(255, 255, 0);
    align-self: center;
}

.popup_text {
    font-size: 40px;
    align-self: center;
    white-space: normal;
    text-overflow: clip;
    -unity-text-align: upper-center;
}

.popup_button {
    height: 90px;
    width: 205px;
    margin-top: 140px;
    margin-left: 50px;
    margin-right: 50px;
    border-top-color: rgb(0, 0, 0);
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
}

.button_cancel {
    background-color: rgb(242, 76, 76);
    color: rgb(255, 255, 255);
}

.button_confirm {
    background-color: rgb(0, 170, 54);
    color: rgb(255, 255, 255);
}
